{
  "scripts": [
    "*/*.py"
  ],
  "vars": {
    "prefix": "/tmp/data/job",
    "job": "RND",
    "__uiHintSourceColumns": [
      "shot",
      "step",
      "pass",
      "type"
    ]
  },
  "tasks":
  [
  {
    "run": "createRenderVersion",
    "metadata": {
      "match.types": [
        "shotRender"
      ],
      "wrapper.name": "python"
    },
    "target": "{prefix}/{job}/sequences/{seq}/{shot}/publish/{step}/{pass}/(newver <parent>)",
    "tasks":
    [
      {
        "run": "nukeTemplate",
        "options": {
          "topLeft": "{shot}",
          "topRight": "(yyyy)(mm)(dd)",
          "notes": "",
          "videocodec": "",
          "bottomLeft": "{pass}, {versionName}",
          "template": "{prefix}/{job}/config/chilopoda/mediaDelivery/templates/template.nk",
          "enableSlate": 0,
          "enableColorSpace": 0,
          "enableReformat": 0,
          "enableTimecode": 1
        },
        "target": "{dataPath}/{name}.mov",
        "metadata": {
          "match.types": [
            "exr"
          ],
          "match.vars": {
            "imageType": [
              "sequence"
            ]
          },
          "wrapper.name": "python"
        },
        "tasks": [
          {
            "run": "appendToVersion",
            "metadata": {
              "wrapper.name": "python"
            }
          }
        ]
      }
    ]
  }
]
}
